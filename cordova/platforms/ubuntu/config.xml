<?xml version='1.0' encoding='utf-8'?>
<widget id="at.oerp.Fpos" version="4.0.30" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="www/img/logo.png" />
    <name>Fpos</name>
    <description>Fpos is a free Point of Sale app with Odoo Sync</description>
    <author email="info@oerp.at" href="http://oerp.at">
        oerp.at
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="true" />
</widget>
